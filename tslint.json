{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "no-console": [true,
      "time",
      "timeEnd",
      "trace"
    ],
    "no-use-before-declare": true,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case",
      "ban-keywords",
      "check-format"
    ]
  }
}